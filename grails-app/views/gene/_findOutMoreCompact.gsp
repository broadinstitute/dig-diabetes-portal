<g:if test="${g.portalTypeString()?.equals('t2d')}">

    <div class="dk-t2d-blue dk-right-column-buttons-compact btn dk-linkout-btn">
    <div class="tooltip-text"><g:message code="findoutmore.entrezgene.descr" default="Explore"/></div>
    <a target="_blank" href="http://www.ncbi.nlm.nih.gov/gene?cmd=search&term=<%=geneName%>%5Bsym%5D">Entrez Gene</a>
</div>

<div class="dk-t2d-blue dk-right-column-buttons-compact btn dk-linkout-btn">
    <div class="tooltip-text"><g:message code="findoutmore.genecards.descr" default="Explore"/></div>
    <a target="_blank" href="http://www.genecards.org/cgi-bin/carddisp.pl?gene=<%=geneName%>">GeneCards</a>
</div>

<div class="dk-t2d-blue dk-right-column-buttons-compact btn dk-linkout-btn">
    <div class="tooltip-text"><g:message code="findoutmore.gtex.descr" default="Explore"/></div>
    <a target="_blank" href="http://www.gtexportal.org/home/gene/<%=geneName%>">GTEx</a>

</div>

<div class="dk-t2d-blue dk-right-column-buttons-compact btn dk-linkout-btn">
    <div class="tooltip-text"><g:message code="findoutmore.hgnc.descr" default="Explore"/></div>
    <a target="_blank" href="https://www.genenames.org/cgi-bin/gene_symbol_report?match=<%=geneName%>">HGNC</a>

</div>

<div class="dk-t2d-blue dk-right-column-buttons-compact btn dk-linkout-btn">
    <div class="tooltip-text"><g:message code="findoutmore.omim.descr" default="Explore"/></div>
